<!DOCTYPE html>
<html>
<head>
    <style type="text/css" media="screen">

        html {
            height: 100%;
        }

        body {
            color: #000000;
            font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
            font-family: 'Lucida Sans', 'Lucida Sans Regular', 'Lucida Grande', 'Lucida Sans Unicode', Geneva, Verdana, sans-serif;
            font-family: Arial, sans-serif;
            font-weight: 200;
            font-size: 14pt;
            height: 100%;
            margin: 0;
            min-height: 100%;
            padding: 0;
            width: 100%;
        }

        .header {
            display: flex;
            align-items: flex-end;
            justify-content: space-between;
            margin-bottom: 30px;
        }

            .header .header__text .title {
                font-size: 30pt;
                font-weight: bold;
                margin-bottom: 5px;
            }

            .header .header__text .subtitle {
                font-size: 20pt;
                font-weight: bold;
                margin-bottom: 3px;
            }

            .header .header__text .id {
                font-size: 20pt;
                font-weight: bold;
                color: #003487;
            }

        .section {
            padding-bottom: 20px;
            page-break-inside: avoid
        }

            .section .section__header {
                font-size: 17pt;
                background-color: #efefef;
                color: #003487;
                padding: 10px 20px;
                font-weight: bold;
                margin-bottom: 20px;
            }

            .section .section__content {
                padding: 0 20px;
            }

                .section .section__content .box {
                    margin-bottom: 15px;
                    page-break-inside: avoid;
                }

                    .section .section__content .box .box__title {
                        font-size: 16pt;
                        font-weight: bold;
                        margin-bottom: 10px;
                    }

                    .section .section__content .box .box__content {
                        display: flex;
                        flex-wrap: wrap;
                    }

                        .section .section__content .box .box__content .data {
                            width: 50%;
                            display: flex;
                        }

                        .section .section__content .box .box__content .data--long {
                            width: 100%;
                        }

                        .section .section__content .box .box__content .data .data__label {
                            min-width: 100px;
                            margin-bottom: 10px;
                            margin-right: 10px;
                            font-size: 14pt;
                            color: black;
                            font-weight: 600;
                            line-height: 22pt;
                        }

                        .section .section__content .box .box__content .data .data__res {
                            font-size: 14pt;
                            margin-bottom: 10px;
                            line-height: 22pt;
                        }

        .signature {
            width: 50%;
        }

            .signature .signature__line {
                border-top: 1px solid black;
                width: 70%;
                margin-top: 90px;
            }

            .signature .signature__label {
                margin-top: 10px;
            }

        table {
            border-collapse: collapse;
            width: 100%;
        }

            table td,
            table th {
                border: 1px solid #ddd;
                padding: 9px 8px;
            }

            table th {
                text-align: left;
                background-color: #efefef;
            }

        .table-t--no {
            width: 20px;
            text-align: center;
        }

        .table-t--noData {
            text-align: center;
            line-height: 50pt;
        }
    </style>
</head>
<body>
    @if (Model != null)
    {
        <div class="header">
            <div class="header__text">
                <div class="title">Mazaya</div>
                <div class="subtitle">Offer Proposal</div>
                <div class="id">ID: 329942</div>
            </div>
            <div class="header__logo">
                <img style="width: 130px;"
                     src="data:image/png;base64," />
            </div>

        </div>

        <div class="section">
            <div class="section__header">
                Company Details
            </div>
            <div class="section__content">
                <div class="box">
                    <div class="box__title">Company Name</div>
                    <div class="box__content">
                        <div class="data data--long">
                            <div class="data__label">Arabic:</div>
                            @if (Model.Company.NameArabic != null)
                            {
                                <div class="data__res">@Model.Company.NameArabic</div>
                            }
                            else
                            {
                                <div class="data__res">--</div>
                            }
                        </div>
                        <div class="data data--long">
                            <div class="data__label">English:</div>
                            @if (Model.Company.NameEnglish != null)
                            {
                                <div class="data__res">@Model.Company.NameEnglish</div>
                            }
                            else
                            {
                                <div class="data__res">--</div>
                            }
                        </div>
                    </div>
                </div>
                <div class="box">
                    <div class="box__title">Contact</div>
                    <div class="box__content">
                        <div class="data">
                            <div class="data__label">Mobile:</div>
                            @if (Model.Company.Mobile != null && Model.Company.Mobile.InternationalNumber != null)
                            {
                                <div class="data__res">@Model.Company.Mobile.InternationalNumber</div>
                            }
                            else
                            {
                                <div class="data__res">--</div>
                            }
                        </div>
                        <div class="data">
                            <div class="data__label">Land:</div>
                            @if (Model.Company.Land != null && Model.Company.Land.InternationalNumber != null)
                            {
                                <div class="data__res">@Model.Company.Land.InternationalNumber</div>
                            }
                            else
                            {
                                <div class="data__res">--</div>
                            }
                        </div>
                        <div class="data">
                            <div class="data__label">Fax:</div>
                            @if (Model.Company.Fax != null && Model.Company.Fax.InternationalNumber != null)
                            {
                                <div class="data__res">@Model.Company.Fax.InternationalNumber</div>
                            }
                            else
                            {
                                <div class="data__res">--</div>
                            }
                        </div>
                        <div class="data">
                            <div class="data__label">P.O.Box:</div>
                            @if (Model.Company.POBox != null)
                            {
                                <div class="data__res">@Model.Company.POBox</div>
                            }
                            else
                            {
                                <div class="data__res">--</div>
                            }
                        </div>
                        <div class="data">
                            <div class="data__label">Website:</div>
                            @if (Model.Company.Website != null || !string.IsNullOrEmpty(Model.Company.Website))
                            {
                                <div class="data__res">@Model.Company.Website</div>
                            }
                            else
                            {
                                <div class="data__res">--</div>
                            }
                        </div>
                        <div class="data">
                            <div class="data__label">Official Email:</div>
                            @if (Model.Company.OfficialEmail != null)
                            {
                                <div class="data__res">@Model.Company.OfficialEmail</div>
                            }
                            else
                            {
                                <div class="data__res">--</div>
                            }
                        </div>
                        <div class="data">
                            <div class="data__label">Facebook:</div>
                            @if (Model.Company.Facebook != null)
                            {
                                <div class="data__res">@Model.Company.Facebook</div>
                            }
                            else
                            {
                                <div class="data__res">--</div>
                            }
                        </div>
                        <div class="data">
                            <div class="data__label">Twiter:</div>
                            @if (Model.Company.Twitter != null)
                            {
                                <div class="data__res">@Model.Company.Twitter</div>
                            }
                            else
                            {
                                <div class="data__res">--</div>
                            }
                        </div>
                        <div class="data">
                            <div class="data__label">Company's Nationality:</div>
                            @if (Model.Company.CompanyNationality != null)
                            {
                                <div class="data__res">@Model.Company.CompanyNationality</div>
                            }
                            else
                            {
                                <div class="data__res">--</div>
                            }
                        </div>
                        <div class="data">
                            <div class="data__label">Establish Date:</div>
                            @if (Model.Company.EstablishDate != null)
                            {
                                <div class="data__res">@Model.Company.EstablishDate.ToString("dd/MM/yyyy HH:mm")</div>
                            }
                            else
                            {
                                <div class="data__res">--</div>
                            }
                        </div>
                        <div class="data">
                            <div class="data__label">Expiry Date:</div>
                            @if (Model.Company.ExpiryDate != null)
                            {
                                <div class="data__res">@Model.Company.ExpiryDate.ToString("dd/MM/yyyy HH:mm")</div>
                            }
                            else
                            {
                                <div class="data__res">--</div>
                            }
                        </div>
                    </div>
                </div>
                <div class="box">
                    <div class="box__title">Partners</div>
                    <div class="box__content">
                        @if (Model.Company.CompanyPartnersList.Count != 0 && Model.Company.CompanyPartnersList[0] != null && Model.Company.CompanyPartnersList[0] != "")
                        {
                            @for (int i = 1; i < Model.Company.CompanyPartnersList.Count + 1; i++)
                            {
                                @if (Model.Company.CompanyPartnersList[i - 1] != null && Model.Company.CompanyPartnersList[i - 1] != "")
                                {
                                    <div class="data">
                                        <div class="data__label">Partner @i:</div>
                                        <div class="data__res">@Model.Company.CompanyPartnersList[i - 1]</div>
                                    </div>
                                }

                            }
                        }
                        else
                        {
                            <div>None of the partners are defined</div>
                        }

                    </div>
                </div>
                <div class="box">
                    <div class="box__title">Commercial Activities</div>
                    <div class="box__content">
                        @if (Model.Company.CompanyActivitiesList.Count != 0 && Model.Company.CompanyActivitiesList[0] != null && Model.Company.CompanyActivitiesList[0] != "")
                        {
                            @for (int i = 1; i < Model.Company.CompanyActivitiesList.Count + 1; i++)
                            {
                                @if (Model.Company.CompanyActivitiesList[i - 1] != null && Model.Company.CompanyActivitiesList[i - 1] != "")
                                {
                                    <div class="data">
                                        <div class="data__label">Activity @i:</div>
                                        <div class="data__res">@Model.Company.CompanyActivitiesList[i - 1]</div>
                                    </div>
                                }

                            }
                        }
                        else
                        {
                            <div>None of the activities are defined</div>
                        }
                    </div>
                </div>
                <div class="box">
                    <div class="box__title">Address and Branches</div>
                    <div class="box__content">
                        @if (Model.Company.CompanyLocations != null)
                        {
                            @for (int i = 1; i < Model.Company.CompanyLocations.Count + 1; i++)
                            {
                                @if (Model.Company.CompanyLocations[i - 1] != null)
                                {
                                    <div class="data data--long">
                                        <div class="data__label">Address @i:</div>
                                        <div class="data__res">@Model.Company.CompanyLocations[i - 1].Address</div>
                                    </div>
                                }

                            }
                        }
                        else
                        {
                            <div>Company doesn't have any addresses.</div>
                        }

                    </div>
                </div>
                <div class="box">
                    <div class="box__title">Focal Point</div>
                    <div class="box__content">
                        <table id="table--focalPoints">
                            <tr>
                                <th class="table-t--no">No.</th>
                                <th>Focal Point Name</th>
                                <th>Title</th>
                                <th>Mobile Number</th>
                            </tr>
                            @if (Model.Company.Suppliers != null && Model.Company.Suppliers.Count > 0)
                            {
                                @for (int i = 1; i < Model.Company.Suppliers.Count + 1; i++)
                                {

                                    <tr>
                                        <td class="table-t--no">@i</td>
                                        <td>@Model.Company.Suppliers[i - 1].FirstName @Model.Company.Suppliers[i - 1].LastName</td>
                                        <td>@Model.Company.Suppliers[i - 1].Title</td>
                                        <td>--</td>
                                    </tr>

                                }
                            }
                            else
                            {
                                <tr>
                                    <td colspan="4" class="table-t--noData">No Focal Points</td>
                                </tr>
                            }
                        </table>
                    </div>
                </div>
            </div>
        </div>
        <div class="section">
            <div class="section__header">
                Discount Info
            </div>
            <div class="section__content">
                <div class="box">
                    <div class="box__title">Vouchers / Coupons</div>
                    <div class="box__content">
                        <table id="table--discount">
                            <tr>
                                <th class="table-t--no">No.</th>
                                <th>Quantity</th>
                                <th>Validity</th>
                                <th>Details</th>
                            </tr>
                            @if (Model.RoadshowVouchers != null && Model.RoadshowVouchers.Count > 0)
                            {
                                @for (int i = 1; i < Model.RoadshowVouchers.Count + 1; i++)
                                {

                                    <tr>
                                        <td class="table-t--no">@i</td>
                                        <td>@Model.RoadshowVouchers[i - 1].Quantity</td>
                                        <td>@Model.RoadshowVouchers[i - 1].Validity</td>
                                        <td>@Model.RoadshowVouchers[i - 1].Details</td>
                                    </tr>

                                }
                            }
                            else
                            {
                                <tr>
                                    <td colspan="4" class="table-t--noData">No Discounts</td>
                                </tr>
                            }

                        </table>
                    </div>
                </div>

            </div>
        </div>
        <div class="section">
            <div class="section__header">
                Terms & Conditions
            </div>
            <div class="section__content">
                <div class="box">
                    <div class="box__content">
                        @if (Model.TermsAndCondition != null)
                        {
                            <div class="text">@Model.TermsAndCondition</div>
                        }
                        else
                        {
                            <div class="text">Not defined</div>
                        }
                    </div>
                </div>
            </div>
        </div>
        <div class="section">
            <div class="section__header">
                Date & Signature
            </div>
            <div class="section__content">
                <div class="box">
                    <div class="box__content">
                        <div class="data data">
                            <div class="data__label">Offer Effective Date:</div>
                            @if (Model.OfferEffectiveDate != null)
                            {
                        <div class="data__res">@Model.OfferEffectiveDate.ToString("dd/MM/yyyy HH:mm")</div>
                            }
                            else
                            {
                                <div class="data__res">--</div>
                            }
                        </div>
                        <div class="data data">
                            <div class="data__label">Name:</div>
                            @if (Model.Name != null)
                            {
                                <div class="data__res">@Model.Name</div>
                            }
                            else
                            {
                                <div class="data__res">--</div>
                            }
                        </div>
                        <div class="data data">
                            <div class="data__label">Expiry Date:</div>
                            @if (Model.ExpiryDate != null)
                            {
                        <div class="data__res">@Model.ExpiryDate.ToString("dd/MM/yyyy HH:mm")</div>
                            }
                            else
                            {
                                <div class="data__res">--</div>
                            }
                        </div>
                        <div class="data data">
                            <div class="data__label">Title</div>
                            @if (Model.Title != null)
                            {
                                <div class="data__res">@Model.Title</div>
                            }
                            else
                            {
                                <div class="data__res">--</div>
                            }
                        </div>
                        <div class="signature">
                            <div class="signature__line"></div>
                            <div class="signature__label">Signature</div>
                        </div>
                        <div class="signature">
                            <div class="signature__line"></div>
                            <div class="signature__label">Recreation Services & Public <br /> Relations Department Manager</div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    }
</body>
</html>
